@model HRIS.Models.Biometric
@{
    Layout = null;
}


@RenderPage("Navigation_top.cshtml")

<script src="https://unpkg.com/gijgo@1.9.11/js/gijgo.min.js" type="text/javascript"></script>
<link href="https://unpkg.com/gijgo@1.9.11/css/gijgo.min.css" rel="stylesheet" type="text/css" />
<link href="~/css/Employee.css" rel="stylesheet" />
<link href="https://fonts.googleapis.com/css?family=Ubuntu" rel="stylesheet">
<link href="https://fonts.googleapis.com/css?family=Source+Sans+Pro" rel="stylesheet">

<div class="container-fluid">
    <br />
        <div class="row">
            <div class="col-sm-3 col-md-6 col-lg-4">
               
                <div class="shadow p-3 mb-5 bg-white rounded">
                    @using (Html.BeginForm())
                    {
                        <h3>Employee ID Number: </h3>
                        @Html.EditorFor(model => model.UserID, new { htmlAttributes = new { @class = "form-control" } })

                        <h3>Start Date: </h3><input id="startDate" />
                        <h3>End Date: </h3><input id="endDate" /> <br>
                        <div class="text-right">

                            <button type="submit" class="btn btn-info" style="width:100px;">FILTER</button>
                        </div>
                    }
                    <br>
                    @RenderPage("Biolist.cshtml")
